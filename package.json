{
  "name": "presentations",
  "scripts": {
    "script:new:slide": "sh scripts/workflow_for_slides/workflow_add_slide.sh",
    "script:remove:slide": "sh scripts/workflow_for_slides/workflow_remove_slide.sh",
    "marp:help": "marp -h",
    "slidev:help": "playwright open https://sli.dev/builtin/cli",
    "playwright:help": "playwright --help",
    "marp:generate:java_and_spring_boot_document": "marp src/marp/java_and_spring_boot_document -o output/marp/java_and_spring_boot_document/JavaとSpring_Bootのお役立ちドキュメント.html -w --html --allow-local-files",
    "marp:generate:sample": "marp src/marp/sample/sample.md -o output/marp/sample/sample.html -w --html --allow-local-files",
    "slidev:open:sample": "slidev src/slidev/sample/sample.md --open",
    "slidev:build:sample": "slidev build src/slidev/sample/sample.md --out out",
    "slidev:export:sample": "slidev export src/slidev/sample/sample.md"
  },
  "dependencies": {
    "@iconify-json/logos": "^1.2.4",
    "@iconify-json/twemoji": "^1.2.2",
    "@iconify-json/uim": "^1.2.3",
    "@marp-team/marp-cli": "^4.0.4",
    "@slidev/cli": "^0.51.0-beta.4",
    "@slidev/theme-apple-basic": "^0.25.1",
    "@slidev/theme-default": "^0.25.0",
    "@slidev/theme-seriph": "^0.25.0",
    "playwright-chromium": "^1.49.1",
    "slidev-theme-academic": "^2.1.0",
    "slidev-theme-eloc": "^1.0.2",
    "slidev-theme-purplin": "^1.2.0",
    "slidev-theme-the-unnamed": "^0.0.22"
  }
}
